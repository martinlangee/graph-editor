<UserControl x:Class="GraphEditor.Nodes.Ui.LogicalXOR_ctrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:nodes="clr-namespace:GraphEditor.Nodes"
             mc:Ignorable="d"
             MinWidth="300" MinHeight="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <nodes:ConfigUiHeader x:Name="Header"/>
        <StackPanel Grid.Row="1" Background="White">
            <TextBlock Text=">> Logical XOR config >>" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="50"/>
        </StackPanel>
    </Grid>
</UserControl>
